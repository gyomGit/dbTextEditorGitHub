	field(DESC, "Display previous transition in T_NO (PRE")
	field(ZNAM, "OFF")
	field(ONAM, "ON")
	field(OUT, "@asynMask(CmdVarFrame 9 0x08)UINT8C")
	field(DTYP, "asynUInt32Digital")
	field(HIGH, "0.075")
	field(SIOL, "$(PREFIX)TSMG-CTRL:T_PREV-calc.VAL CP MS")
	field(SIML, "$(PREFIX)SIMMODE CP NMS")
	field(PINI, "YES")
}
